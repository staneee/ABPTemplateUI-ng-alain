{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src/",
    "paths": {
        "@abp":["../node_modules/abp-ng2-module/src/"],
        "@abp/*":["../node_modules/abp-ng2-module/src/*"],
        "@shared": [ "shared" ],
        "@shared/*": [ "shared/*" ],
        "@core": [ "app/core/" ],
        "@core/*": [ "app/core/*" ],
        "@testing": [ "testing/" ],
        "@testing/*": [ "testing/*" ],
        "@env": [ "environments/" ],
        "@env/*": [ "environments/*" ]
    }
  }
}
